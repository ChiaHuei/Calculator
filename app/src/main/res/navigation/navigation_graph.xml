<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/myNameFragment">

    <fragment
        android:id="@+id/myNameFragment"
        android:name="com.example.myapplication.MyNameFragment"
        android:label="fragment_my_name"
        tools:layout="@layout/fragment_my_name" >
        <action
            android:id="@+id/action_myNameFragment_to_calculatorFragment"
            app:destination="@id/calculatorFragment" />
    </fragment>
    <fragment
        android:id="@+id/calculatorFragment"
        android:name="com.example.myapplication.CalculatorFragment"
        android:label="fragment_calculator"
        tools:layout="@layout/fragment_calculator" />
</navigation>